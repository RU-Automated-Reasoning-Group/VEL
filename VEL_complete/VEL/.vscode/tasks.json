{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: g++-8 build active file",
            "command": "/usr/bin/g++-8",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "-w",
                "-L../flowstar/flowstar-toolbox",
                "-L/usr/local/lib",
                "${file}",
                "-lflowstar",
                "-lmpfr",
                "-lgmp",
                "-lgsl",
                "-lgslcblas",
                "-lm",
                "-lglpk",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}